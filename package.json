    "express-validator": "^6.14.0",
    "http-proxy-middleware": "^2.0.3",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.2.3",
    "shortid": "^2.2.16"
  },
  "devDependencies": {
    "concurrently": "^7.0.0",
